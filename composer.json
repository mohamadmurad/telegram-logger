{
  "name": "mohamadmurad/telegram-logger",
  "type": "library",
  "description": "send laravel log to telegram",
  "keywords": [
    "laravel",
    "telegram",
    "logger",
    "report"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "monolog/monolog": "^2.5|^3.3",
    "irazasyed/telegram-bot-sdk": "^3.13"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "TelegramLogger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TelegramLogger\\TelegramLoggerServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "mohamad murad",
      "email": "mhdite7@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "@php vendor/bin/testbench package:discover --ansi"
    ]
  }
}
